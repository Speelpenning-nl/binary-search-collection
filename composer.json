    "require": {
        "illuminate/support": ">=5.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.5",
        "phpunit/phpunit": "~4.8",
        "symfony/stopwatch": "^2.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gerben Speelpenning",
            "email": "gerben@speelpenning.nl"
        }
